      <children>
            <Button fx:id="loginButton" layoutX="237.0" layoutY="166.0" mnemonicParsing="false" onAction="#loginButtonClicked" text="Login" />
            <Label layoutX="129.0" layoutY="93.0" text="User Name :" />
            <Label layoutX="137.0" layoutY="128.0" text="Password :" />
            <TextField fx:id="userNameTextField" layoutX="201.0" layoutY="89.0" />
            <PasswordField fx:id="passwordTextField" layoutX="201.0" layoutY="124.0" />
      </children>
    </AnchorPane>
  </children>
</VBox>
