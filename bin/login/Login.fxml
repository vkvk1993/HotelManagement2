<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login.LoginController">
  <children>
    <AnchorPane fx:id="loginAndchorPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="loginButton" layoutX="266.0" layoutY="195.0" mnemonicParsing="false" onAction="#loginButtonClicked" text="Login" />
            <TextField fx:id="userNameTextField" layoutX="214.0" layoutY="106.0" promptText="User ID" />
            <PasswordField fx:id="passwordTextField" layoutX="214.0" layoutY="146.0" promptText="Password" />
      </children>
    </AnchorPane>
  </children>
</VBox>
